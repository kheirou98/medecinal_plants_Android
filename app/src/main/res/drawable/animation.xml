<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >

            <corners android:radius="100dp" ></corners>
            <padding android:bottom="5pt" android:left="5pt" android:top="5pt"></padding>
            <stroke android:color="#c900ff10"  android:width="7dp"></stroke>

        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >

            <corners android:radius="9dp" ></corners>
            <padding android:bottom="1pt" android:left="1pt" android:top="1pt"></padding>
            <stroke android:color="#FF6D0000"  android:width="5dp"></stroke>
        </shape>
    </item>

</selector>